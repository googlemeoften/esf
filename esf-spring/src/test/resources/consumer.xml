<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans-3.2.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-3.2.xsd">

    <bean id="helloWorldService" class="cn.edu.esf.spring.ESFSpringConsumerBean">
        <property name="group" value="ESF"/>
        <property name="version" value="1.0.0"/>
        <property name="interfaceName" value="cn.edu.esf.HelloService"/>
    </bean>

    <bean id="helloservice" class="cn.edu.esf.spring.ESFSpringConsumerBean">
        <property name="group" value="ESF"/>
        <property name="version" value="1.0.0"/>
        <property name="interfaceName" value="cn.edu.esf.HelloService2"/>
    </bean>
</beans>